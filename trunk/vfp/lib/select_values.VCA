PROCEDURE KeyPress
LPARAMETERS nKeyCode, nShiftAltCtrl
	DO CASE 
	CASE nKeyCode=13
	*энтер
		DODEFAULT()
		THISFORM.okevent()
	CASE nKeyCode=22
	*инсерт
		THISFORM.insert_item()
		NODEFAULT
 	CASE nkeyCode=-2
 	*поиск следующей
		DODEFAULT()
 		THISFORM.seekVal("NEXT")
	CASE ISALPHA(CHR(nkeyCode)) OR ISDIGIT(CHR(nkeyCode)) OR (nKeyCode=6 AND nShiftAltCtrl=2)
 	*поиск
 		LOCAL lcseekform as String
 		*
		DODEFAULT()
		KEYBOARD CHR(nkeyCode)
		*
		lcseekform=NEWOBJECT("selseek", "..\lib\select_values", "", THISFORM.left,THISFORM.top,"Поиск значения",THISFORM.whoSeek,THISFORM)
		*
		SET CURSOR ON
		lcseekform.show()
		SET CURSOR OFF
	OTHERWISE
	ENDCASE
ENDPROC
