WITH This
	IF .Enabled
    	.imgBtn2.Visible = .T.
    	.imgBtn1.Visible = .F.
	ENDIF 
ENDWITH 
ENDPROC
PROCEDURE MouseUp
LPARAMETERS nButton, nShift, nXCoord, nYCoord
WITH This
	.imgBtn2.Visible = .T.
	.imgBtn1.Visible = .F.
	.cmdHide.Visible = .T.
ENDWITH 
ENDPROC
PROCEDURE Refresh
IF This.Enabled
	WITH this
	    .imgBtn1.Visible = .F.
    	.imgBtn2.Visible = .F.
	    .imgBtn3.Visible = .F.
    	.imgBtn4.Visible = .F.
    	.imgBtn5.Visible = .F.
    	*
	    .cmdHide.Enabled = .T.
   		.lblText.Enabled = .T.
   	ENDWITH 
ELSE 
	WITH this
	    .imgBtn1.Visible = .F.
    	.imgBtn2.Visible = .F.
	    .imgBtn3.Visible = .F.
    	.imgBtn4.Visible = .T.
    	.imgBtn5.Visible = .F.
    	*
	    .cmdHide.Enabled = .F.
    	.lblText.Enabled = .F.
    ENDWITH 
ENDIF
ENDPROC
